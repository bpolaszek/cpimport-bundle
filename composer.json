{
    "name": "bentools/cpimport-bundle",
    "description": "Symfony Bundle - Use file watchers with MariaDb Columnstore CpImport utility",
    "keywords": ["mariadb columnstore", "cpimport", "php", "symfony", "file watcher"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "symfony/config": "~3.0||~4.0",
        "symfony/console": "~3.0||~4.0",
        "symfony/dependency-injection": "~3.0||~4.0",
        "symfony/http-kernel": "~3.0||~4.0",
        "symfony/lock": "~3.0||~4.0",
        "symfony/process": "~3.0||~4.0",
        "symfony/yaml": "~3.0||~4.0"
    },
    "require-dev": {
        "symfony/dotenv": "~4.0",
        "symfony/framework-bundle": "~3.0||~4.0",
        "symfony/var-dumper": "~4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "BenTools\\CpImportBundle\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
            "vendor/symfony/var-dumper/Resources/functions/dump.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
